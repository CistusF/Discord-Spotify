{
  "private": true,
  "name": "discord-spotify",
  "version": "1.3.0",
  "description": "A simple system to show your current playing song in Spotify as your playing status in Discord.",
  "main": "bot.js",
  "scripts": {
    "start": "cd dist && node bot.js",
    "start-dev": "./node_modules/.bin/nodemon --exec babel-node bot.js",
    "test": "./node_modules/.bin/standard && ./node_modules/.bin/eslint **",
    "build": "babel . --ignore node_modules,scripts,.github,README.md,config.example.json --out-dir dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LWTechGaming/Discord-Spotify.git"
  },
  "author": "LWTechGaming",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/LWTechGaming/Discord-Spotify/issues"
  },
  "homepage": "https://github.com/LWTechGaming/Discord-Spotify#readme",
  "dependencies": {
    "discordie": "github:qeled/discordie#dev",
    "spotify-web-helper": "^1.7.1",
    "winston": "^2.3.1"
  },
  "devDependencies": {
    "babel-cli": "^6.24.0",
    "babel-preset-es2015": "^6.24.0",
    "eslint": "^3.19.0",
    "eslint-config-standard": "^7.1.0",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^2.1.1",
    "nodemon": "^1.11.0",
    "standard": "^9.0.2"
  }
}
